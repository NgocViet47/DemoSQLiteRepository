<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mypc.demosqliterepository">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".service.BaseApplication">
        <activity android:name=".activity.main.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.main.UnitActivity" />
        <activity android:name=".activity.main.ClassActivity" />
        <activity android:name=".activity.main.StudentActivity" />
        <activity android:name=".activity.main.TranscriptActivity" />
        <activity android:name=".activity.createactivity.CreateUnitActivity" />
        <activity android:name=".activity.createactivity.CreateClassActivity" />
        <activity android:name=".activity.createactivity.CreateStudentActivity" />
        <activity android:name=".activity.createactivity.CreateTranscriptActivity"></activity>
    </application>

</manifest>